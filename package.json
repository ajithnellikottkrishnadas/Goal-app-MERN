{
  "name": "mern_goalsapp",
  "version": "1.0.0",
  "description": "MERN Goals App",
  "main": "server/server.js",
  "type": "module",

  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon server/server.js",
    "client": "npm start --prefix client",
    "client-install": "npm install --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --prefix client",
    "render-postbuild": "npm install --prefix client && npm run build --prefix client"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ajithnellikottkrishnadas/Goal-app-MERN.git"
  },
  "keywords": [],
  "author": "Ajith",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ajithnellikottkrishnadas/Goal-app-MERN/issues"
  },
  "homepage": "https://github.com/ajithnellikottkrishnadas/Goal-app-MERN#readme",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.0"
  }
}
